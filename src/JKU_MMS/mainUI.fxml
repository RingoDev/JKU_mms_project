<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JKU_MMS.Controller">
    <VBox spacing="5.0">
        <HBox spacing="5.0">
            <TextField fx:id="inputFile">test.mp4</TextField>
            <ChoiceBox fx:id="chooseProfile" prefWidth="150.0">
            </ChoiceBox>
        </HBox>
        <HBox spacing="5.0">
            <Button fx:id="fileChooser">Choose Video</Button>
            <Button fx:id="addTask">Add Task</Button>
            <Button fx:id="process">Process Queue</Button>
        </HBox>
    </VBox>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
